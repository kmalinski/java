<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.LoginController">
  <children>
    <AnchorPane id="pane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="462.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="155.0" layoutY="177.0" prefHeight="27.0" prefWidth="0.0" style="&#10;" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <PasswordField id="passField" layoutX="202.0" layoutY="177.0" prefHeight="55.0" prefWidth="205.0" promptText="Password" />
            <TextField id="usernameField" layoutX="202.0" layoutY="115.0" prefHeight="40.0" prefWidth="205.0" promptText="Username" />
            <Button id="okButton" layoutX="248.0" layoutY="250.0" mnemonicParsing="false" onAction="#login" prefHeight="27.0" prefWidth="96.0" text="Zaloguj" />
            <Button id="closeButton" layoutX="595.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitProgram" text="X" textFill="#ee0606" />
      </children>
    </AnchorPane>
  </children>
